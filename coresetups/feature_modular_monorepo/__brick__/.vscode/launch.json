  "version": "0.2.0",
  "configurations": [
    {
      "name": "dev",
      "request": "launch",
      "type": "dart",
      "program": "app/{{appName.snakeCase()}}/lib/main_dev.dart",
      "args": ["--flavor", "development", "--target", "lib/main_dev.dart"]
    },
    {
      "name": "staging",
      "request": "launch",
      "type": "dart",
      "program": "app/{{appName.snakeCase()}}/lib/main_stg.dart",
      "args": ["--flavor", "staging", "--target", "lib/main_stg.dart"]
    },
    {
      "name": "prod",
      "request": "launch",
      "type": "dart",
      "program": "app/{{appName.snakeCase()}}/lib/main.dart",
      "args": ["--flavor", "production", "--target", "lib/main.dart"]
    },
    {
      "name": "widgetbook",
      "request": "launch",
      "type": "dart",
      "program": "app/{{appName.snakeCase()}}_widgetbook/lib/main.dart",
      "args": ["--target", "lib/main.dart"]
    }
  ]
}
